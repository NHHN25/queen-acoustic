{
  "public": "out",
  "cleanUrls": false,
  "trailingSlash": true,
  "directoryListing": false,
  "rewrites": [{ "source": "/**", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
